### foreman.repo
#
# Foreman repository file
#
# managed by Puppet, local changes will be overwritten
#
[foreman]
name=Foreman 1.7
<%= @elForeman %>
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-foreman

[foreman-source]
name=Foreman 1.7 - source
<%= @elForemanSource %>
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-foreman
