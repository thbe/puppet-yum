### foreman.repo
#
# Foreman repository file
#
# managed by Puppet, local changes will be overwritten
#
[foreman]
name=Foreman 1.4
<%= @el6Foreman %>
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-foreman

[foreman-source]
name=Foreman 1.4 - source
<%= @el6ForemanSource %>
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-foreman
