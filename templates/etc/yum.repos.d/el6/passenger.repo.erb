### passenger.repo
#
# Passenger repository file
#
# managed by Puppet, local changes will be overwritten
#
[passenger]
name = Red Hat Enterprise $releasever - Phusion Passenger
<%= scope.lookupvar("yum::el_passenger") %>
enabled = 1
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-passenger
gpgcheck = 1

[passenger-testing]
name = Red Hat Enterprise $releasever - Phusion Passenger - TEST
<%= scope.lookupvar("yum::el_passenger_testing") %>
enabled = 0
gpgkey = file:///etc/pki/rpm-gpg/RPM-GPG-KEY-passenger
gpgcheck = 0
