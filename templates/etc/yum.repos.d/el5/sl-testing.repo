[sl-testing]
name=Scientific Linux 5 testing area
baseurl=http://ftp.scientificlinux.org/linux/scientific/5rolling/testing/$basearch
		http://ftp1.scientificlinux.org/linux/scientific/5rolling/testing/$basearch
		http://ftp2.scientificlinux.org/linux/scientific/5rolling/testing/$basearch
		ftp://ftp.scientificlinux.org/linux/scientific/5rolling/testing/$basearch
