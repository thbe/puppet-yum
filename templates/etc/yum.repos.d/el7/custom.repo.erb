### custom.repo
#
# custom repository file
#
# managed by Puppet, local changes will be overwritten
#
[custom]
name=Custom Packages for Enterprise Linux $releasever - $basearch
<%= scope.lookupvar("yum::el_custom") %>
failovermethod=priority
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-thbe
priority=11
