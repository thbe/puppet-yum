### owncloud.repo
#
# OwnCloud repository file
#
# managed by Puppet, local changes will be overwritten
#
[isv_ownCloud_community]
name=Latest stable community release of ownCloud (CentOS_CentOS-7)
type=rpm-md
<%= scope['::yum::el_owncloud'] %>
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-owncloud-CentOS-7
enabled=1
